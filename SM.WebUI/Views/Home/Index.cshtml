<script>
    function Edit(id, like) {
        $.ajax({
            type: "post",
            data: { id: id, like: like },
            url: '/Home/Edit'
        }).done(function (result) {
            var html = ""
            for (var i = 0; i < result.length; i++) {
                html += '<tr><td>' + result[i].id + '</td><td>' + result[i].numberOfLikes + '</td><td><button onclick="Edit(' + result[i].id + ',true)">Like</button></td></tr>'
            }
            document.getElementsByClassName("s-content").innerHTML = html;
        });
    }

    function open() {
        $.ajax({
            type: "get",
            data: { id: 'f4fff185-b300-4a1d-a910-3b9d886d4700' },
            url: '/Home/Details'
        }).document.getElementsByClassName
    }
</script>